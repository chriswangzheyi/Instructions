<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wzy.springboot_seckill.dao.GoodsDetailDAO">
    <select id="findByGoodsId" parameterType="long" resultType="com.wzy.springboot_seckill.entity.GoodsDetail">
        select * from t_goods_detail where goods_id = #{value} order by gd_order
    </select>
</mapper>